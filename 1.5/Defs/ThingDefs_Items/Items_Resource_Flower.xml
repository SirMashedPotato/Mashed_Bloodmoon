<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ==========[Mashed_Bloodmoon_WolfsbaneFlower]==========-->
	<ThingDef ParentName="RoughPlantBase">
		<defName>Mashed_Bloodmoon_WolfsbaneFlower</defName>
		<label>wolfsbane flower</label>
		<description>Flowers harvested from a wolfsbane plant, can be used to create wolfsbane powder.\n\nSome explorers and traders will keep a small handful of wolfsbane flowers on them at all times, as Melynx find the smell simply irresistible.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Mashed_Bloodmoon_RawWolfsbane</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.2</MarketValue>
			<Flammability>1.3</Flammability>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>PlantMatter</li>
		</thingCategories>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
		</ingestible>
		<tradeTags Inherit="False"/>
	</ThingDef>
</Defs>
