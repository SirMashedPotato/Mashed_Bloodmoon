<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Bloodmoon_WolfsbanePlantDecorative]==========-->
	<ThingDef ParentName="PlantBase" Name="Mashed_Bloodmoon_WolfsbanePlantBase">
		<defName>Mashed_Bloodmoon_WolfsbanePlantDecorative</defName>
		<label>fools wolfsbane</label>
		<description>A small flowering plant, only found growing in the highest mountains. In theory lycanthropes would have a rather strong reaction to the petals of these flowers, except that this particular strain is purely decorative. It offers nothing of practical use, and is only nice to look at.</description>
		<statBases>
			<Beauty>6</Beauty>
			<BeautyOutdoors>6</BeautyOutdoors>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mashed_Bloodmoon_WolfsbanePlant</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>false</selectable>
		<pathCost>14</pathCost>
		<ingestible/>
		<plant>
			<growDays>6</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<sowTags>
				<li>Ground</li>
				<li>Hydroponic</li>
			</sowTags>
			<topWindExposure>0.1</topWindExposure>
			<maxMeshCount>25</maxMeshCount>
			<visualSizeRange>0.8~1</visualSizeRange>
			<wildOrder>2</wildOrder>
			<sowResearchPrerequisites>
				<li>Mashed_Bloodmoon_Wolfsbane</li>
			</sowResearchPrerequisites>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Bloodmoon_WolfsbanePlant]==========-->
	<ThingDef ParentName="Mashed_Bloodmoon_WolfsbanePlantBase">
		<defName>Mashed_Bloodmoon_WolfsbanePlant</defName>
		<label>wolfsbane plant</label>
		<description>A small flowering plant, only found growing in the highest mountains. Lycanthropes have a rather strong reaction to the petals of these flowers, which can be greatly intensified through certain means.</description>
		<selectable>true</selectable>
		<plant>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>Mashed_Bloodmoon_WolfsbaneFlower</harvestedThingDef>
			<harvestYield>1</harvestYield>
			<sowMinSkill>9</sowMinSkill>
			<maxMeshCount>4</maxMeshCount>
			<visualSizeRange>0.8~1.2</visualSizeRange>
			<topWindExposure>0.3</topWindExposure>
		</plant>
	</ThingDef>
</Defs>
