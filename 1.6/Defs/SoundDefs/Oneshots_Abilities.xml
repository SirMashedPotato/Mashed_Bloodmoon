	<!-- ==========[Mashed_Bloodmoon_AdrenalineRush]==========-->
	<SoundDef>
		<defName>Mashed_Bloodmoon_AdrenalineRush</defName>
		<context>MapOnly</context>
		<maxSimultaneous>3</maxSimultaneous>
		<priorityMode>PrioritizeNearest</priorityMode>
		<subSounds>
			<li>
				<grains>
					<li Class="AudioGrain_Clip">
						<clipPath>Mashed_Bloodmoon_Lycanthrope/mixkit-human-single-heart-beat-490</clipPath>
					</li>
				</grains>
				<volumeRange>25</volumeRange>
				<muteWhenPaused>true</muteWhenPaused>
				<tempoAffectedByGameSpeed>true</tempoAffectedByGameSpeed>
			</li>
		</subSounds>
	</SoundDef>
